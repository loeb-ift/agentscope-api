# 项目基本信息
PROJECT_NAME=AgentScope Multi-Agent Debate API
PROJECT_VERSION=1.0.0
PROJECT_DESCRIPTION=为n8n工作流提供多Agent辩论功能

# 环境配置
ENVIRONMENT=development  # development, staging, production
DEBUG=True
HOST=0.0.0.0
PORT=8000
WORKERS=1

# API配置
API_PREFIX=/api
API_URL=http://localhost:8000

# 数据库配置
DATABASE_URL=postgresql://admin:password@localhost:5432/agentscope
POSTGRES_DB=agentscope
POSTGRES_USER=admin
POSTGRES_PASSWORD=password

# Redis配置
REDIS_URL=redis://localhost:6379/0

# CORS配置
BACKEND_CORS_ORIGINS=["http://localhost", "http://localhost:8080", "http://localhost:3000"]

# 辩论配置
DEFAULT_DEBATE_ROUNDS=3
MAX_DEBATE_ROUNDS=10
DEBATE_STEP_DELAY=2  # Agent响应之间的延迟（秒）
DEBATE_EXPIRY_DAYS=30  # 辩论会话过期天数

# 时间配置
TIMEZONE=Asia/Shanghai

# Celery配置
CELERY_RESULT_EXPIRES=3600  # 结果过期时间（秒）
CELERY_TASK_SOFT_TIME_LIMIT=300  # 任务软时间限制（秒）
CELERY_TASK_TIME_LIMIT=600  # 任务硬时间限制（秒）
CELERY_WORKER_MAX_TASKS_PER_CHILD=10  # 每个工作器最大处理任务数
CELERY_WORKER_PREFETCH_MULTIPLIER=1  # 预取任务乘数

# Webhook配置
WEBHOOK_TIMEOUT=10  # Webhook请求超时时间（秒）

# 模型配置
# 可根据实际使用的模型提供商进行配置
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx